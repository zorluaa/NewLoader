-- Key Validation
local function loadScript(gameName)
    if gameName == "Assassin" then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/zorluaa/Void1/refs/heads/main/15"))()
    elseif gameName == "Murder VS Sheriff Duels" then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/zorluaa/voidmvsd/refs/heads/main/mvsdvoid"))()
    elseif gameName == "Fisch" then
        loadstring(game:HttpGet("https://your-link-to-game3-script"))()
    else
        print("No script available for the selected game.")
    end
end

-- Sound Effects
local buttonSound = Instance.new("Sound")
buttonSound.SoundId = "rbxassetid://YOUR_BUTTON_SOUND_ID"

local keyErrorSound = Instance.new("Sound")
keyErrorSound.SoundId = "rbxassetid://YOUR_ERROR_SOUND_ID"

-- UI Creation
local ScreenGui = Instance.new("ScreenGui")
local MainContainer = Instance.new("Frame")
local KeyPrompt = Instance.new("Frame")
local KeyLabel = Instance.new("TextLabel")
local KeyInput = Instance.new("TextBox")
local SubmitButton = Instance.new("TextButton")
local GameSelectionFrame = Instance.new("Frame")
local Game1Button = Instance.new("TextButton")
local Game2Button = Instance.new("TextButton")
local Game3Button = Instance.new("TextButton")
local UICorner = Instance.new("UICorner")

ScreenGui.Parent = game.CoreGui

-- Main Container
MainContainer.Parent = ScreenGui
MainContainer.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
MainContainer.Size = UDim2.new(0, 600, 0, 350)
MainContainer.Position = UDim2.new(0.5, -300, 0.5, -175)
MainContainer.BackgroundTransparency = 0.3
MainContainer.BorderSizePixel = 0

local containerUICorner = UICorner:Clone()
containerUICorner.Parent = MainContainer

-- Key Prompt Section
KeyPrompt.Parent = MainContainer
KeyPrompt.BackgroundColor3 = Color3.fromRGB(44, 44, 58)
KeyPrompt.Size = UDim2.new(0, 600, 0, 150)
KeyPrompt.Position = UDim2.new(0, 0, 0, 0)
KeyPrompt.BackgroundTransparency = 0.1

local keyLabel = KeyLabel
keyLabel.Parent = KeyPrompt
keyLabel.Size = UDim2.new(1, 0, 0, 50)
keyLabel.Position = UDim2.new(0, 0, 0, 10)
keyLabel.Font = Enum.Font.GothamBold
keyLabel.Text = "Enter Key: VoidLua"
keyLabel.TextSize = 24
keyLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
keyLabel.TextAlignment = Enum.TextAlignment.Center

local keyInput = KeyInput
keyInput.Parent = KeyPrompt
keyInput.BackgroundColor3 = Color3.fromRGB(55, 55, 70)
keyInput.Size = UDim2.new(0, 500, 0, 40)
keyInput.Position = UDim2.new(0.5, -250, 0, 70)
keyInput.Font = Enum.Font.Gotham
keyInput.TextColor3 = Color3.fromRGB(255, 255, 255)
keyInput.TextSize = 18
keyInput.ClearTextOnFocus = false
keyInput.Text = ""

-- Submit Button
SubmitButton.Parent = KeyPrompt
SubmitButton.BackgroundColor3 = Color3.fromRGB(58, 58, 74)
SubmitButton.Size = UDim2.new(0, 200, 0, 50)
SubmitButton.Position = UDim2.new(0.5, -100, 0, 120)
SubmitButton.Font = Enum.Font.GothamBold
SubmitButton.Text = "Submit"
SubmitButton.TextSize = 18
SubmitButton.TextColor3 = Color3.fromRGB(255, 255, 255)
SubmitButton.AutoButtonColor = false
SubmitButton.BackgroundTransparency = 0.2
local submitUICorner = UICorner:Clone()
submitUICorner.Parent = SubmitButton

-- Animation Functions
local function fadeOutPrompt()
    KeyPrompt:TweenSize(UDim2.new(0, 600, 0, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Quart, 0.5, true)
end

local function showGameSelection()
    GameSelectionFrame:TweenSize(UDim2.new(0, 600, 0, 200), Enum.EasingDirection.Out, Enum.EasingStyle.Quart, 0.5, true)
end

-- Game Selection Frame
GameSelectionFrame.Parent = ScreenGui
GameSelectionFrame.BackgroundColor3 = Color3.fromRGB(36, 36, 51) -- Dark Purple
GameSelectionFrame.Size = UDim2.new(0, 600, 0, 0)
GameSelectionFrame.Position = UDim2.new(0, 0, 0, 150)
GameSelectionFrame.BackgroundTransparency = 0.3
GameSelectionFrame.Visible = false

local gameSelectionUICorner = UICorner:Clone()
gameSelectionUICorner.Parent = GameSelectionFrame

-- Game Buttons
Game1Button.Parent = GameSelectionFrame
Game1Button.BackgroundColor3 = Color3.fromRGB(55, 55, 70)
Game1Button.Size = UDim2.new(0, 500, 0, 50)
Game1Button.Position = UDim2.new(0.5, -250, 0, 10)
Game1Button.Font = Enum.Font.GothamBold
Game1Button.Text = "Assassin"
Game1Button.TextSize = 20
Game1Button.TextColor3 = Color3.fromRGB(255, 255, 255)
local buttonUICorner1 = UICorner:Clone()
buttonUICorner1.Parent = Game1Button

Game2Button.Parent = GameSelectionFrame
Game2Button.BackgroundColor3 = Color3.fromRGB(55, 55, 70)
Game2Button.Size = UDim2.new(0, 500, 0, 50)
Game2Button.Position = UDim2.new(0.5, -250, 0, 70)
Game2Button.Font = Enum.Font.GothamBold
Game2Button.Text = "Murder VS Sheriff Duels"
Game2Button.TextSize = 20
Game2Button.TextColor3 = Color3.fromRGB(255, 255, 255)
local buttonUICorner2 = UICorner:Clone()
buttonUICorner2.Parent = Game2Button

Game3Button.Parent = GameSelectionFrame
Game3Button.BackgroundColor3 = Color3.fromRGB(55, 55, 70)
Game3Button.Size = UDim2.new(0, 500, 0, 50)
Game3Button.Position = UDim2.new(0.5, -250, 0, 130)
Game3Button.Font = Enum.Font.GothamBold
Game3Button.Text = "Fisch"
Game3Button.TextSize = 20
Game3Button.TextColor3 = Color3.fromRGB(255, 255, 255)
local buttonUICorner3 = UICorner:Clone()
buttonUICorner3.Parent = Game3Button

-- Button Hover Effects
local function buttonHover(button)
    button.MouseEnter:Connect(function()
        button.BackgroundColor3 = Color3.fromRGB(75, 75, 90)
    end)
    button.MouseLeave:Connect(function()
        button.BackgroundColor3 = Color3.fromRGB(55, 55, 70)
    end)
end

buttonHover(Game1Button)
buttonHover(Game2Button)
buttonHover(Game3Button)

-- Key Validation & Button Functionality
SubmitButton.MouseButton1Click:Connect(function()
    if KeyInput.Text == "VoidLua" then
        keyErrorSound:Stop()
        buttonSound:Play()
        KeyLabel.Text = "Key Accepted! Loading..."
        fadeOutPrompt()
        wait(0.5)
        showGameSelection()
    else
        keyErrorSound:Play()
        KeyLabel.Text = "Incorrect Key! Try Again."
        KeyInput.Text = ""
    end
end)

Game1Button.MouseButton1Click:Connect(function()
    loadScript("Assassin")
    ScreenGui:Destroy() -- Destroy the loader after selection
end)

Game2Button.MouseButton1Click:Connect(function()
    loadScript("Murder VS Sheriff Duels")
    ScreenGui:Destroy() -- Destroy the loader after selection
end)

Game3Button.MouseButton1Click:Connect(function()
    loadScript("Fisch")
    ScreenGui:Destroy() -- Destroy the loader after selection
end)

